@using DynamicView.Example.Models
@model StudentTeacherViewModel

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div>
        @foreach (var item in Model.Students)
        {
            @item.Name
            <br />
        }
        <hr />
        @foreach (var item in Model.Teachers)
        {
            @item.Name
            <br />
        }
    </div>
</body>
</html>
